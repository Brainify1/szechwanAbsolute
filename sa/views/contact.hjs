    <!-- Page-->
    <div class="page text-center">
      <!-- Page Header-->
      <header class="page-head section-top-15 section-lg-top-0">
        <!-- RD Navbar-->
        {{> header}}
        <section class="section-top-50 section-bottom-66 section-lg-top-160 section-lg-bottom-160 inset-left-15 inset-right-15 bg-about">
          <div class="header-divider">
            <h3 class="text-uppercase font-logo text-regular letter-spacing-200">Contact</h3>
                  <div class="text-center offset-top-9">
                    <ol class="breadcrumb">
                      <li><a href="./">Home</a></li>
                      <li>Contact us</li>
                    </ol>
                  </div>
          </div>
        </section>
      </header>
      <main class="page-content">
        <section class="section-top-50 section-bottom-83 section-sm-top-70 section-sm-bottom-83 text-center text-sm-left">
          <div class="shell">
            <h3 class="text-center">Get in touch</h3>
            <div class="range range-xs-center offset-top-32">
              <div class="cell-sm-12 cell-md-10 cell-lg-8">
                <div class="range">
                  <div class="cell-sm-4 text-center text-sm-left">
                    <div>
                      <img class="imgCenter" src="../images/classic-phone.png">
                    </div>
                    <div class="infoBox">
                      <p class="infoBoxP">Phone<br>(917)983-6666</p>
                    </div>
                  </div>
                  <div class="cell-sm-4 text-center text-sm-left">
                    <div>
                      <img class="imgCenter" src="../images/building.png">
                    </div>
                    <div class="infoBox">
                      <p class="infoBoxP">Address:<br>39-16 Prince St(2 FL)<br>Flushing NY 11354</p>
                    </div>
                  </div>
                  <div class="cell-sm-4 text-center text-sm-left">
                    <div>
                      <img class="imgCenter" src="../images/envelope.png">
                    </div>
                    <div class="infoBox">
                      <p class="infoBoxP">Email:</p>
                    </div>
                  </div>
                  <div class="cell-xs-12 offset-top-37">
                    <!-- RD Google Map-->
                    <div id="contact-map" class="map-area">
                      <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3022.1198364210695!2d-73.83485688497547!3d40.75938894265273!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c2600e42a1f5f5%3A0xf949f31e3da7e344!2s39-16+Prince+St%2C+Flushing%2C+NY+11354!5e0!3m2!1sen!2sus!4v1499284399569" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>
                  </div>
                  <div class="cell-xs-12 offset-top-42">
                    <hr class="hr-gray hr-fullwidth">
                  </div>
                </div>
              </div>
            </div>
          </div>
        </section>
      </main>
      {{> footer}}
    </div>
